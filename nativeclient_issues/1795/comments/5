win7-32-bare-newlib-opt [small_tests] [96 seconds]
win7-32-bare-glibc-opt  [small_tests] [86 seconds]

win7-64-bare-newlib-opt [small_tests] [134 seconds]
win7-64-bare-glibc-opt  [small_tests] [128 seconds]

