look like the one below.
Most of the nops seem redundant.

  20340:	55                   	push   %rbp
   20341:	90                   	nop
   20342:	90                   	nop
  
  
   2035e:	90                   	nop
   2035f:	90                   	nop
   20360:	48 89 e5             	mov    %rsp,%rbp
   20363:	90                   	nop

 
   2037d:	90                   	nop
   2037e:	90                   	nop
   2037f:	90                   	nop
   20380:	83 ec 30             	sub    $0x30,%esp
   20383:	4c 01 fc             	add    %r15,%rsp
   20386:	90                   	nop
   20387:	90                   	nop
