Look for my &lt;&lt;&lt; markers

======================================================

LBB7_17:                               # %lpad   &lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt; LANDING PAD ENTRY
.Ltmp28:
	movl	%eax, 28(%esp)
	movl	%edx, 24(%esp)
.LBB7_18:                               # %ppad46
	cmpl	$2, 24(%esp)
	je	.LBB7_4
	jmp	.LBB7_9
	.align	32, 0x90

===================================
corresponding table
====================================

GCC_except_table7:
.Lexception7:
	.byte	255                     # @LPStart Encoding = omit
	.byte	0                       # @TType Encoding = absptr
	.byte	133                     # @TType base offset
	.byte	129
	.zero	2,128
	.zero	1
	.byte	3                       # Call site Encoding = udata4
	.uleb128	117             # Call site table length
.Lset0 = .Leh_func_begin7-.Leh_func_begin7 # Region start
	.long	.Lset0
.Lset1 = .Ltmp12-.Leh_func_begin7       # Region length
	.long	.Lset1
	.long	0                       # Landing pad
	.uleb128	0               # Action
.Lset2 = .Ltmp12-.Leh_func_begin7       # Region start
	.long	.Lset2
.Lset3 = .Ltmp13-.Ltmp12                # Region length
	.long	.Lset3
.Lset4 = .Ltmp28-.Leh_func_begin7       # Landing pad    &lt;&lt;&lt;&lt; EFFECTIVELY takes                      
                                        #                   address of that BBL
	.long	.Lset4
	.uleb128	5               # Action
.Lset5 = .Ltmp14-.Leh_func_begin7       # Region start
	.long	.Lset5
.Lset6 = .Ltmp15-.Ltmp14                # Region length
	.long	.Lset6




