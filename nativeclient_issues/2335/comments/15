$  wget http://gsdview.appspot.com/nativeclient-archive2/toolchain/7252/naclsdk_linux_x86.tgz -O- -o/dev/null| tar xSvpf naclsdk_linux_x86.tgz | grep include/.*nacl.*h | grep -v 'c[+][+]'
sdk/nacl-sdk/x86_64-nacl/include/sys/nacl_imc_api.h
sdk/nacl-sdk/x86_64-nacl/include/sys/nacl_nice.h
sdk/nacl-sdk/x86_64-nacl/include/sys/nacl_name_service.h
sdk/nacl-sdk/x86_64-nacl/include/sys/nacl_kern_rpc.h
sdk/nacl-sdk/x86_64-nacl/include/sys/nacl_syscalls.h
sdk/nacl-sdk/x86_64-nacl/include/bits/nacl_imc_api.h
sdk/nacl-sdk/x86_64-nacl/include/bits/nacl_syscalls.h
