			<ns2:uri>/u/bradnelson@google.com/</ns2:uri><ns2:username>bradnelson@google.com</ns2:username></ns2:cc><ns2:state>open</ns2:state><ns0:title>Incremental Windows Gyp build does not rebuild trusted .S files when .h files change</ns0:title><ns2:label>Type-Defect</ns2:label><ns2:label>Pri-2</ns2:label><ns2:label>Component-Infrastructure</ns2:label><ns2:label>Component-TCB</ns2:label><ns2:label>OS-Windows</ns2:label><ns2:label>Gyp</ns2:label><ns0:link href="http://code.google.com/feeds/issues/p/nativeclient/issues/2969/comments/full" rel="replies" type="application/atom+xml" /><ns0:link href="http://code.google.com/p/nativeclient/issues/detail?id=2969" rel="alternate" type="text/html" /><ns0:link href="https://code.google.com/feeds/issues/p/nativeclient/issues/full/2969" rel="self" type="application/atom+xml" /><ns2:stars>1</ns2:stars><ns2:id>2969</ns2:id></ns0:entry>